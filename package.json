{
  "name": "phaser3-webpack-template",
  "version": "1.0.0",
  "description": "Phaser 3 project template for Phaser Editor 2D v3",
  "main": "webpack.config.js",
  "scripts": {
    "build:server": "tsc -p src/server/tsconfig.json",
    "serve:server": "nodemon src/server/server.ts",
    "start": "webpack serve --mode=development --stats-error-details",
    "build": "webpack --mode=production && phaser-asset-pack-hashing -r dist",
    "editor": "phasereditor2d-launcher -project . -port 1959",
    "editor-remote": "phasereditor2d-launcher -project . -disable-open-browser -public -port 1959",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Phaser Editor 2D <developers@phasereditor2d.com> (https://phasereditor2d.com/)",
  "license": "MIT",
  "dependencies": {
    "colyseus.js": "^0.14.13",
    "phaser": "^3.55.2",
    "@geckos.io/phaser-on-nodejs": "^1.2.5",
    "colyseus": "^0.14.23",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "typescript": "^4.4.3",
    "@colyseus/monitor": "^0.14.22"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^6.8.0",
    "minimist": ">=1.2.2",
    "typescript": "^4.2.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.9.1",
    "colyseus.js": "^0.14.13",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.0",
    "ts-node": "^10.2.1",
    "mocha": "^9.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.0",
    "html-webpack-plugin": "^5.3.1",
    "phaser-asset-pack-hashing": "^1.0.5",
    "phasereditor2d-launcher": "^3.32.0",
    "ts-loader": "^8.0.18",
    "webpack": "^5.28.0",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  }
}
